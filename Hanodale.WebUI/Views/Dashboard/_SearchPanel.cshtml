@*@model Hanodale.WebUI.Models.AssetTypeStockModel*@

    <article class="col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable">

        <!-- Widget ID (each widget will need unique ID)-->

        <!-- end widget -->

        <div class="jarviswidget jarviswidget-sortable" id="wid-id-1150" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-custombutton="false" style="" role="widget">
            <!-- widget div-->
            <div role="content">

                <!-- widget edit box -->
                <div class="jarviswidget-editbox">
                    <!-- This area used as dropdown edit box -->

                </div>
                <!-- end widget edit box -->

                <!-- widget content -->
                <div class="widget-body no-padding">


                    <fieldset>
                        <div class="smart-form">
                            @*<legend>@Resources.PAGE_HEADER_CREATE_USER</legend>*@


                            <div class="form-group">
                                @Html.LabelFor(model => model.assetType_Id, new { @class = "col-xs-12 col-sm-3 col-md-3 col-lg-2 control-label", })

                                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-5">
                                    <div class="clearfix">
                                        <label class="select">
                                            @Html.EditorFor(model => model.assetType_Id, new { @class = "", dataValue = Model.lstAssetType })
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.status_Id, new { @class = "col-xs-12 col-sm-3 col-md-3 col-lg-2 control-label", })

                                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-5">
                                    <div class="clearfix">
                                        <label class="select">
                                            @Html.EditorFor(model => model.status_Id, new { @class = "", dataValue = Model.lstStatus })
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </fieldset>
                </div>
                <!-- end widget content -->
            </div>
            <!-- end widget div -->
        </div>
    </article>