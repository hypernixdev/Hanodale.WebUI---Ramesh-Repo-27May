    <script type="text/javascript">

        $('a#show-shortcut').click(function () {
            var container = $("#main > #content");
            var _businessType = $(this).attr("data-businessType");
            if (_businessType == 65) {
                $.ajax({
                    type: "POST",
                    datatype: 'json',
                    url: "User/UserInfo/",
                    data: {},
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        container.html("");
                        container.html(data.viewMarkup);
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        //log error
                        //_showErrorMessage();
                    }
                });
            }

        });

        // DO NOT REMOVE : GLOBAL FUNCTIONS!

        $(document).ready(function () {

            pageSetUp();
            $('.multipleSelect1').select2();
            $('#nav-search').show();

        })

        $('#organization_Id').data('lastSelected', $('#organization_Id').find("option:selected"));
        $('#mainCostCenter_Id').data('lastSelected', $('#mainCostCenter_Id').find("option:selected"));

    </script>


